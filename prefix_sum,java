class PrefixSum {
    public static void main(String[] args) {
        int[] nums = {2, 4, 5, 3};
        int n = nums.length;

        int[] prefix = new int[n];
        prefix[0] = nums[0];

        for (int i = 1; i < n; i++) {
            prefix[i] = prefix[i - 1] + nums[i];
        }

      
        for (int x : prefix) {
            System.out.print(x + " ");
        }
    }
}
